generator client {
  provider = "prisma-client-js"
}

datasource db{
    provider = "mysql"
    url = env("DATABASE_URL")
    relationMode = "prisma"
}

enum bug_types{
    bug
    suggestion
}

model bug_reports {
  id    Int          @id @default(autoincrement())
  created_at         DateTime @default(now())
  bug_type           bug_types
  description String @db.VarChar(2000)
}